{
  "name": "salat-saom-api",
  "version": "1.0.0",
  "description": "Salat and Saom Timing API for Bangladesh",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "node src/database/migrate.js",
    "migrate:status": "node src/database/migrate.js status",
    "migrate:rollback": "node src/database/migrate.js rollback",
    "seed": "node src/database/seed.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint . --ext .js",
    "format": "prettier --write \"src/**/*.js\"",
    "prepare": "husky install"
  },
  "keywords": [
    "prayer-times",
    "fasting-times",
    "islamic",
    "bangladesh",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "mysql2": "^3.6.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "axios": "^1.6.2",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
